#: templates/registrationinternal.template.txt
msgid "Send form mail"
msgstr "Wyślij formularz pocztą"

#: templates/registrationinternal.template.txt
msgid "Form on public website"
msgstr "Formularz na stronie publicznej"

#: templates/registrationinternal.template.txt
msgid "The form has not yet been published."
msgstr "Formularz nie został jeszcze opublikowany."

#: templates/registrationinternal.template.txt
msgid "Please create a form first."
msgstr "Najpierw utwórz formularz."

#: templates/registrationinternal.template.txt
msgid "Please add email templates for the form."
msgstr "Prosimy o dodanie szablonów wiadomości e-mail dla formularza."

